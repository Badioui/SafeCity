<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Couleur quand le Chip est coché (Traité) -->
    <item android:color="@android:color/holo_green_dark" android:state_checked="true" />

    <!-- Couleur quand le Chip n'est pas coché (Nouveau) -->
    <item android:color="@android:color/holo_blue_dark" android:state_checked="false" />

    <!-- Couleur par défaut -->
    <item android:color="@android:color/darker_gray" />
</selector>